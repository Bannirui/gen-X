<idea-plugin>
    <id>com.x.gen-X</id>
    <name>Gen-X</name>

    <vendor email="bannirui@outlook.com">X</vendor>

    <description><![CDATA[
    just provide a simple access to standard template
  ]]></description>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <projectTemplatesFactory implementation="com.x.genx.factory.TemplateFactory"/>
        <applicationService serviceImplementation="com.x.genx.service.MyMavenProjectDataLoad"/>
    </extensions>
</idea-plugin>