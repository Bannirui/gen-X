<idea-plugin>
    <id>com.x.gen-X</id>
    <name>Gen-X</name>

    <vendor email="bannirui@outlook.com" url="https://www.cnblogs.com/miss-u/">Bannirui</vendor>

    <description><![CDATA[
    just provide a simple access to standard template
  ]]></description>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <projectTemplatesFactory implementation="com.github.bannirui.genx.factory.TemplateFactory"/>
        <applicationService serviceImplementation="com.github.bannirui.genx.service.SpringSettings"/>
    </extensions>
</idea-plugin>